{
  "debug": false,
  "mediaWarnings": false,
  "input": [
    "path/to/project/markup/folder"
  ],
  "output": "path/to/styles/output/folder",
  "variables": {
    "css": {
      "enabled": "false"
    },
    "sass": {
      "enabled": "false"
    }
  },
  "borderWeights": [
    {
      "key": "1px",
      "weight": "1px"
    },
    {
      "key": ".5rem",
      "weight": ".5rem"
    },
    {
      "key": "1rem",
      "weight": "1rem"
    }
  ],
  "colors": [
    {
      "key": "transparent",
      "color": "rgba(255, 255, 255, 0)",
      "variants": {
        "enabled": "false"
      },
      "opacities": []
    },
    {
      "key": "white",
      "color": "rgba(255, 255, 255, 1)",
      "variants": {
        "enabled": "false"
      },
      "opacities": [
        ".2"
      ]
    },
    {
      "key": "black",
      "color": "rgba(20, 20, 30, 1)",
      "variants": {
        "enabled": "false"
      },
      "opacities": [
        ".8"
      ]
    },
    {
      "key": "primary",
      "color": "#9D5CFF",
      "variants": {
        "enabled": "true",
        "light": "#d7bdff",
        "dark": "#36106e"
      },
      "opacities": [
        ".1"
      ]
    },
    {
      "key": "secondary",
      "color": "#5CFFEE",
      "variants": {
        "enabled": "false"
      },
      "opacities": []
    },
    {
      "key": "accent",
      "color": "#FF5C6D",
      "variants": {
        "enabled": "false"
      },
      "opacities": []
    }
  ],
  "containers": [
    {
      "key": "s",
      "maxWidth": "39rem"
    },
    {
      "key": "m",
      "maxWidth": "60rem"
    },
    {
      "key": "l",
      "maxWidth": "80rem"
    }
  ],
  "flexgrid": {
    "enabled": false,
    "columns": 12
  },
  "fonts": [
    {
      "key": "sans",
      "loadType": "system",
      "value": "sans-serif",
      "url": null
    },
    {
      "key": "serif",
      "loadType": "system",
      "value": "serif",
      "url": null
    },{
      "key": "script",
      "loadType": "system",
      "value": "script",
      "url": null
    },
    {
      "key": "mono",
      "loadType": "system",
      "value": "monospace",
      "url": null
    }
  ],
  "fontBaseSize": "112.5%",
  "fontScale": "1.25",
  "gradients": [],
  "heights": [
    {
      "key": "100",
      "value": "100%"
    },
    {
      "key": "75",
      "value": "75%"
    },
    {
      "key": "50",
      "value": "50%"
    },
    {
      "key": "25",
      "value": "25%"
    },
    {
      "key": "0",
      "value": "0"
    }
  ],
  "layerCount": 5,
  "media": [
    {
      "key": "xs",
      "value": "screen and (min-width: 48em)"
    },
    {
      "key": "s",
      "value": "screen and (min-width: 55em)"
    },
    {
      "key": "m",
      "value": "screen and (min-width: 64em)"
    },
    {
      "key": "l",
      "value": "screen and (min-width: 100em)"
    }
  ],
  "opacities": [
    {
      "key": "0",
      "value": "0"
    },
    {
      "key": ".5",
      "value": ".5"
    },
    {
      "key": "1",
      "value": "1"
    }
  ],
  "radius": [
    {
      "key": "rounded",
      "value": "10px"
    },
    {
      "key": "pill",
      "value": "50rem"
    },{
      "key": "circle",
      "value": "100%"
    }
  ],
  "shadows": [
    {
      "key": "xs",
      "value": "0 0.1rem 0.2rem 0 rgba(0, 0, 0, .2)"
    },
    {
      "key": "s",
      "value": "0 0.25rem 0.5rem -0.05rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "m",
      "value": "0 0.4rem 0.7rem -0.1rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "l",
      "value": "0 0.55rem 1rem -0.2rem rgba(0, 0, 0, .2)"
    },
    {
      "key": "xl",
      "value": "0 0.7rem 1.5rem -0.3rem rgba(0, 0, 0, .2)"
    }
  ],
  "whitespaceScale": "1.5",
  "widths": [
    {
      "key": "100",
      "value": "100%"
    },
    {
      "key": "75",
      "value": "75%"
    },
    {
      "key": "50",
      "value": "50%"
    },
    {
      "key": "25",
      "value": "25%"
    },
    {
      "key": "0",
      "value": "0"
    }
  ]
}