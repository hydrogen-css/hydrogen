{
  "input": [
    "markup"
  ],
  "output": [
    "styles"
  ],
  "colors": [
    {
      "key": "primary",
      "color": "purple",
      "variants": {
        "light": false,
        "dark": false
      },
      "opacities": []
    }
  ],
  "containers": [
    {
      "key": "s",
      "maxWidth": "39rem"
    },
    {
      "key": "m",
      "maxWidth": "60rem"
    },
    {
      "key": "l",
      "maxWidth": "80rem"
    }
  ],
  "fonts": [
    {
      "key": "sans",
      "family": "sans-serif"
    }
  ],
  "baseFontSizes": [
    {
      "size": "100%",
      "query": "screen"
    },
    {
      "size": "112.5%",
      "query": "screen and (min-width: 64em)"
    },{
      "size": "118%",
      "query": "screen and (min-width: 100em)"
    }
  ],
  "baseLineHeight": "1.3",
  "typeScale": "1.25",
  "gradients": [
    {
      "key": "myLinearGradient",
      "type": "linear",
      "angle": "135deg",
      "colorStops": [
        "green",
        "pink",
        "rgba(255, 255, 255, 1)"
      ]
    },
    {
      "key": "myRadialGradient",
      "type": "radial",
      "colorStops": [
        "green",
        "pink",
        "rgba(255, 255, 255, 1)"
      ]
    }
  ],
  "media": [
    {
      "key": "b",
      "query": null
    },
    {
      "key": "p",
      "query": "print"
    },
    {
      "key": "xs",
      "query": "screen and (min-width: 48em)"
    },
    {
      "key": "s",
      "query": "screen and (min-width: 55em)"
    },
    {
      "key": "m",
      "query": "screen and (min-width: 64em)"
    },
    {
      "key": "l",
      "query": "screen and (min-width: 100em)"
    }
  ],
  "radius": [
    {
      "key": "rounded",
      "radius": "10px"
    }
  ],
  "shadows": [
    {
      "key": "xs",
      "shadow": "0 0.1rem 0.2rem 0 rgba(0, 0, 0, .2)"
    }
  ]
}