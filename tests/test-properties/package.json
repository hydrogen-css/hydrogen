{
  "name": "@hydrogen-css/hydrogen-tests-properties",
  "version": "1.0.0",
  "scripts": {
    "browsersync": "browser-sync start --server --watch --startPath markup/index.html --reload-delay 500ms --no-open",
    "watch": "npx h2-watch",
    "watch-logs": "npx h2-watch --logs",
    "watch-clean": "npx h2-watch --clean",
    "serve": "npm-run-all --parallel watch browsersync",
    "serve-logs": "npm-run-all --parallel watch-logs browsersync",
    "serve-clean": "npm-run-all --parallel watch-clean browsersync"
  },
  "devDependencies": {
    "browser-sync": "^2.27.10",
    "npm-run-all": "^4.1.5"
  }
}